{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && node build-utils.js",
    "lint": "eslint ./src --quiet",
    "preview": "vite preview",
    "server": "node src/server.js",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "deploy": "gh-pages -d dist",
    "db:setup": "psql -U postgres -d gatoscallejeros -f src/database/schema.sql && psql -U postgres -d gatoscallejeros -f src/database/seed.sql",
    "supabase:create-tables": "node src/database/createSupabaseTables.js",
    "supabase:create-exec-sql": "node src/database/createExecSQLFunction.js",
    "supabase:verify-exec-sql": "node src/database/verifyExecSQLFunction.js",
    "supabase:create-rpc": "node src/database/createRPCFunctions_fixed.js",
    "supabase:direct-sql": "node src/database/runDirectSQL.js",
    "supabase:setup-rls": "node src/database/setupRLS.js",
    "supabase:upload-users": "node src/database/uploadUsersToSupabase.js",
    "supabase:upload-users-alt": "node src/database/uploadUsersToSupabaseAlt.js",
    "supabase:upload-cats": "node src/database/uploadCatsToSupabase.js",
    "supabase:test-connection": "node src/database/testSupabaseConnection.js",
    "supabase:test-basic": "node src/database/testBasicConnection.js",
    "supabase:test-login": "node src/database/testLogin.js",
    "supabase:test-simple-login": "node src/database/testSimpleLogin.js",
    "supabase:consultar-usuario": "node src/database/consultarUsuario.js",
    "supabase:listar-usuarios": "node src/database/listarUsuarios.js",
    "supabase:crear-usuario-elena": "node src/database/crearUsuarioElena.js",
    "supabase:configurar-rls": "node src/database/configurarRLS.js",
    "supabase:test-direct-login": "node src/database/testDirectLogin.js"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^6.0.2",
    "@supabase/supabase-js": "^2.47.12",
    "@types/prop-types": "^15.7.14",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "gh-pages": "^6.3.0",
    "globals": "^15.9.0",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.10",
    "vite": "^5.4.1"
  }
}
